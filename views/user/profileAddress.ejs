    <script>
        $(document).ready(function () {
            $('#saveAddress').click(function () {
                var isValid = true;

                $('.text-danger').text('');

                var fullName = $('#fullName').val().trim();
                if (fullName.length < 3) {
                    $('#fullNameError').text('Full Name must be at least 3 characters.');
                    isValid = false;
                }

                var email = $('#email').val().trim();
                var emailPattern = /^[a-zA-Z0-9._%+-]+@gmail\.com$/;
                if (!emailPattern.test(email)) {
                    $('#emailError').text('Please enter a valid Gmail address.');
                    isValid = false;
                }

                var phone = $('#phone').val().trim();
                var phonePattern = /^\d{10}$/;
                if (!phonePattern.test(phone)) {
                    $('#phoneError').text('Phone number must be exactly 10 digits.');
                    isValid = false;
                }

                var pinCode = $('#pinCode').val().trim();
                var pinCodePattern = /^\d{6}$/;
                if (!pinCodePattern.test(pinCode)) {
                    $('#pinCodeError').text('Please give valid pin code.');
                    isValid = false;
                }

                var streetAddress = $('#streetAddress').val().trim();
                if (streetAddress.length < 10) {
                    $('#streetAddressError').text('Give currect street address.');
                    isValid = false;
                }

                var apartmentNumber = $('#apartmentNumber').val().trim();
                if (apartmentNumber !== '' && (!/^\d{1,3}$/.test(apartmentNumber) || parseInt(apartmentNumber) > 999)) {
                    $('#apartmentNumberError').text('Provide valid appartment number');
                    isValid = false;
                }

                var town = $('#town').val().trim();
                if (!/^[a-zA-Z\s]+$/.test(town)) {
                    $('#townError').text('Give valid town.');
                    isValid = false;
                }

                var city = $('#city').val().trim();
                if (!/^[a-zA-Z\s]+$/.test(city)) {
                    $('#cityError').text('Provide currect city .');
                    isValid = false;
                }

                var state = $('#state').val().trim();
                if (!/^[a-zA-Z\s]+$/.test(state)) {
                    $('#stateError').text('Provide true state .');
                    isValid = false;
                }

                if (isValid) {
                    var formData = $('#addAddressForm').serialize();
                    $.ajax({
                        url: '/add-address',
                        type: 'POST',
                        data: formData,
                        success: function (response) {
                            if (response.success) {
                                $('#addAddressModal').modal('hide');
                                location.reload();
                            } else {
                                alert('Failed to add address. Please try again.');
                            }
                        },
                        error: function () {
                            alert('An error occurred. Please try again.');
                        }
                    });
                }
            });
        });
    </script>


    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script>
        $(document).ready(function () {
            $('.edit-address').on('click', function () {
                const addressId = $(this).data('id');
                const addressCard = $(this).closest('.card-body');

                $('#editAddressId').val(addressId);
                $('#editFullName').val(addressCard.find('p').text().match(/Full Name: (.*)/)[1]);
                $('#editStreetAddress').val(addressCard.find('p').text().match(/Street Address: (.*)/)[1]);
                $('#editApartmentNumber').val(addressCard.find('p').text().match(/Apartment Number: (.*)/)[1]);
                $('#editCity').val(addressCard.find('p').text().match(/City: (.*)/)[1]);
                $('#editTown').val(addressCard.find('p').text().match(/Town: (.*)/)[1]);
                $('#editPinCode').val(addressCard.find('p').text().match(/Pin Code: (.*)/)[1]);
                $('#editPhone').val(addressCard.find('p').text().match(/Phone: (.*)/)[1]);

                $('#editAddressModal').modal('show');
            });

            $('#updateAddressBtn').on('click', function () {
                const formData = $('#editAddressForm').serialize();

                $.ajax({
                    url: '/update-address',
                    method: 'POST',
                    data: formData,
                    success: function (response) {
                        if (response.success) {
                            $('#editAddressModal').modal('hide');
                            location.reload();
                        } else {
                            alert('Failed to update address. Please try again.');
                        }
                    },
                    error: function () {
                        alert('An error occurred. Please try again.');
                    }
                });
            });
        });
    </script>

    <!-- delete address -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const deleteButtons = document.querySelectorAll('.delete-address');

            deleteButtons.forEach(button => {
                button.addEventListener('click', function (e) {
                    e.preventDefault();
                    const addressId = this.getAttribute('data-id');

                    Swal.fire({
                        title: 'Are you sure?',
                        text: "Cancel",
                        icon: 'warning',
                        showCancelButton: true,
                        confirmButtonColor: '#3085d6',
                        cancelButtonColor: '#d33',
                        confirmButtonText: 'Yes Delete '
                    }).then((result) => {
                        if (result.isConfirmed) {
                            deleteAddress(addressId);
                        }
                    });
                });
            });
        });

        function deleteAddress(addressId) {
            fetch(`/delete-address/${addressId}`, {
                method: 'DELETE',
            })
                .then(response => response.json())
                .then(data => {
                    if (data.success) {
                        Swal.fire(
                            'Deleted!',
                            'Your address has been deleted.',
                            'success'
                        ).then(() => {
                            location.reload();
                        });
                    } else {
                        Swal.fire(
                            'Error!',
                            'Failed to delete the address.',
                            'error'
                        );
                    }
                })
                .catch(error => {
                    console.error('Error:', error);
                    Swal.fire(
                        'Error!',
                        'An error occurred while deleting the address.',
                        'error'
                    );
                });
        }
    </script>

